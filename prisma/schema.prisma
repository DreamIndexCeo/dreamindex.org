generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type AdmissionsBookedDate {
  Day   Int
  Month Int
  Year  Int
}

type AdmissionsMadeDate {
  Day   Int
  Month Int
  Year  Int
}

type FeedbackCreated {
  Day   Int
  Month Int
  Year  Int
}

type FormsQuestions {
  one FormsQuestionsOne
}

type FormsQuestionsOne {
  respones String
  subject  String
}

model Admissions {
  id         String               @id @default(auto()) @map("_id") @db.ObjectId
  BookedDate AdmissionsBookedDate
  BookedTime String
  MadeDate   AdmissionsMadeDate
  business   String
  email      String
  name       String
  phone      Int
  status     String
}

model Clients {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  business      String
  email         String
  ip            String
  name          String
  password      String
  phone         Int
  plan          String
  root_password String  @map("root-password")
  status        Boolean
  website       String
}

model Feedback {
  id      String          @id @default(auto()) @map("_id") @db.ObjectId
  Created FeedbackCreated
  Field   String
  Status  Boolean
  Subject String
  userID  String          @db.ObjectId
}

model Forms {
  id         String         @id @default(auto()) @map("_id") @db.ObjectId
  Business   String
  Email      String
  Name       String
  Phone      String
  Questions  FormsQuestions
  accessCode String
}

model Request {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  clientid  String
  date_made DateTime @db.Date
  details   String
  resolved  DateTime @db.Date
  status    Boolean
  subject   String
  warning   Boolean
}

model Team {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
